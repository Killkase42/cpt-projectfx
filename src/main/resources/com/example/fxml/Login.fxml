<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="background-login" stylesheets="@/com/example/css/styles.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.LoginController">
   <children>
      <Pane fx:id="secondaryPane" layoutX="262.0" layoutY="170.0" styleClass="foreground-login" stylesheets="@/com/example/css/styles.css">
         <children>
            <Label layoutX="63.0" layoutY="14.0" prefHeight="53.0" prefWidth="121.0" styleClass="text-login" stylesheets="@/com/example/css/styles.css" text="User Login" textAlignment="CENTER">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <TextField fx:id="UsernameTextField" layoutX="45.0" layoutY="66.0" promptText="Enter Your Username" styleClass="username-text-field" stylesheets="@/com/example/css/styles.css" />
            <PasswordField fx:id="PasswordTextField" layoutX="45.0" layoutY="101.0" promptText="Enter Your Password" styleClass="password-text-field" stylesheets="@/com/example/css/styles.css" />
            <TextField fx:id="ShowPasswordTextField" layoutX="45.0" layoutY="101.0" promptText="Enter Your Password" styleClass="password-show-text-field-login" stylesheets="@/com/example/css/styles.css" visible="false" />
            <Button fx:id="LoginButton" layoutX="71.0" layoutY="149.0" mnemonicParsing="false" onAction="#Validate" prefHeight="29.0" prefWidth="102.0" styleClass="button-login" stylesheets="@/com/example/css/styles.css" text="Login" />
            <Button fx:id="ToCreateAccountButton" layoutX="71.0" layoutY="178.0" mnemonicParsing="false" onAction="#ToCreateAccount" prefHeight="25.0" prefWidth="102.0" styleClass="button-to-create-account" stylesheets="@/com/example/css/styles.css" text="Create Account" textAlignment="CENTER" underline="true" />
            <Button fx:id="ShowPasswordButton" layoutX="193.0" layoutY="101.0" mnemonicParsing="false" onAction="#ShowPassword" styleClass="button-to-show-password-login" stylesheets="@/com/example/css/styles.css" text="Show" />
            <Label fx:id="ErrorMessage" layoutX="77.0" layoutY="130.0" prefHeight="17.0" prefWidth="99.0" styleClass="error-message-login" stylesheets="@/com/example/css/styles.css" text="Fields Left Empty" textAlignment="CENTER" visible="false" />
            <Label fx:id="ErrorMessage1" layoutX="67.0" layoutY="130.0" prefHeight="17.0" prefWidth="119.0" styleClass="error-message-login-1" stylesheets="@/com/example/css/styles.css" text="Incorrect Credentials" textAlignment="CENTER" visible="false" />
         </children></Pane>
   </children>
</AnchorPane>
