<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.CreateNewAccountController">
   <children>
      <Accordion />
      <Label layoutX="208.0" layoutY="51.0" prefHeight="39.0" prefWidth="183.0" text="Create New Account" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="UsernameTextField" layoutX="224.0" layoutY="146.0" promptText="Enter Your Username" />
      <TextField fx:id="PasswordTextField" layoutX="224.0" layoutY="200.0" promptText="Enter Your Password" />
      <Button fx:id="CreateAccountButton" layoutX="247.0" layoutY="272.0" mnemonicParsing="false" onAction="#CreateAccount" text="Create Account" />
      <Button fx:id="MainMenuButton" layoutX="259.0" layoutY="336.0" mnemonicParsing="false" onAction="#GoToMainMenu" text="Main Menu" />
   </children>
</AnchorPane>
